{
  "name": "04-clean-ddd",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "start": "node build/server.js",
    "build": "tsup",
    "lint": "eslint . --max-warnings 0 --config eslint.config.mjs",
    "lint:cache": "npm run lint -- --cache --fix",
    "pretty": "prettier . --write --cache --cache-location=./.prettiercache",
    "precommit": "lint-staged",
    "type-check": "tsc --noEmit",
    "prepare": "husky",
    "test": "vitest --project unit run",
    "test:e2e": "vitest --project e2e run",
    "test:all": "vitest run",
    "test:watch": "vitest --project unit --watch",
    "test:e2e:watch": "vitest --project e2e --watch",
    "test:coverage": "vitest --project unit --coverage",
    "test:ui": "vitest --ui",
    "test:coverage:ui": "vitest --coverage --ui"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "Rocketseat ignite clean code and DDD",
  "dependencies": {
    "@fastify/cookie": "11.0.2",
    "@fastify/jwt": "9.0.4",
    "@prisma/client": "6.5.0",
    "bcryptjs": "3.0.2",
    "date-fns": "4.1.0",
    "dotenv": "16.4.7",
    "fastify": "5.2.1",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.0",
    "@commitlint/config-conventional": "19.8.0",
    "@eslint/js": "9.22.0",
    "@faker-js/faker": "9.6.0",
    "@types/bcryptjs": "2.4.6",
    "@types/node": "22.13.10",
    "@types/supertest": "6.0.2",
    "@vitest/coverage-v8": "3.0.8",
    "@vitest/ui": "3.0.8",
    "eslint": "9.22.0",
    "eslint-config-prettier": "10.1.1",
    "eslint-import-resolver-typescript": "3.8.4",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-prettier": "5.2.3",
    "eslint-plugin-vitest": "0.5.4",
    "globals": "16.0.0",
    "husky": "9.1.7",
    "lint-staged": "15.4.3",
    "prettier": "3.5.3",
    "prisma": "6.5.0",
    "supertest": "7.0.0",
    "tsup": "8.4.0",
    "tsx": "4.19.3",
    "typescript": "5.8.2",
    "typescript-eslint": "8.26.1",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.0.8"
  }
}
